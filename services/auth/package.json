{
  "name": "@sst-app/auth",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "default": "./index.ts",
      "types": "./index.ts"
    },
