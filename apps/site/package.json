{
  "name": "site",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i app/styles/app.css -o app/styles/app-compiled.css",
    "dev": "concurrently \"npm run dev:css\" \"sst-env -- remix dev\"",
    "dev:css": "tailwindcss -w -i app/styles/app.css -o app/styles/app-compiled.css",
    "lint": "TIMING=1 eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "@remix-run/eslint-config",
      "@remix-run/eslint-config/node",
      "@sst-app/eslint-config-custom"
    ],
    "root": true
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.7",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "@remix-run/serve": "^1.6.8",
    "@sst-app/auth": "*",
    "@sst-app/trpc-mux": "*",
    "@trpc/client": "10.0.0-alpha.47",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.9.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.8",
    "@remix-run/eslint-config": "^1.6.8",
    "@serverless-stack/static-site-env": "^1.6.6",
    "@sst-app/eslint-config-custom": "*",
    "@sst-app/tsconfig": "*",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/node": "^16",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.3.0",
    "tailwindcss": "^3.1.8"
  }
}
