{
  "name": "site",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i app/styles/app.css -o app/styles/app-compiled.css",
    "dev": "concurrently \"npm run dev:css\" \"sst-env -- remix dev\"",
    "dev:css": "tailwindcss -w -i app/styles/app.css -o app/styles/app-compiled.css",
    "lint": "TIMING=1 eslint . --ext=.ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint -- --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@sst-app/eslint-config-custom"
    ],
    "rules": {
      "@typescript-eslint/ban-types": "off",
      "@typescript-eslint/consistent-type-imports": "off",
      "turbo/no-undeclared-env-vars": "off"
    },
    "root": true
  },
  "dependencies": {
    "@remix-run/node": "^1.7.1",
    "@remix-run/react": "^1.7.1",
    "@remix-run/serve": "^1.7.1",
    "@sst-app/trpc-mux": "*",
    "@trpc/client": "10.0.0-proxy-beta.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.10.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.1",
    "@serverless-stack/static-site-env": "^1.6.6",
    "@sst-app/eslint-config-custom": "*",
    "@sst-app/tsconfig": "*",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^16",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.4.0",
    "tailwindcss": "^3.1.8"
  }
}
