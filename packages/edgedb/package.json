{
  "name": "@sst-app/edgedb",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/microeinhundert/sst-edgedb-trpc-microservices.git",
    "directory": "packages/edgedb"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "link": "sh scripts/link.sh",
    "create-migration": "edgedb migration create",
    "migrate": "edgedb migrate",
    "generate-query-builder": "sh scripts/generate-query-builder.sh",
    "lint": "TIMING=1 eslint . --ext=.ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint -- --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@sst-app/eslint-config-custom"
    ],
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off"
    },
    "root": true
  },
  "dependencies": {
    "@serverless-stack/node": "^1.15.16",
    "@sst-app/lambda-utils": "*",
    "edgedb": "^1.0.0"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.0.3",
    "@sst-app/eslint-config-custom": "*",
    "@sst-app/tsconfig": "*",
    "@types/node": "^16"
  }
}
