      "@typescript-eslint/ban-ts-comment": "off"
    },
    "root": true
  },
  "devDependencies": {
    "@sst-app/eslint-config-custom": "*",
    "@sst-app/tsconfig": "*",
    "@types/node": "^16"
  }
}
