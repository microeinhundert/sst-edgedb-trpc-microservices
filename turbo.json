{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "typecheck": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "link": {
      "cache": false
    },
    "create-migration": {
      "dependsOn": ["link"],
      "cache": false
    },
    "migrate": {
      "dependsOn": ["link"],
      "cache": false
    },
    "generate-query-builder": {
      "dependsOn": ["link"],
      "cache": false
    }
  }
}
